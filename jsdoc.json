{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "template": "template/docdash-with-search/",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "search": true,
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false
    },
    "scripts": [],
    "favicon": "",
    "css": []
  },
  "openGraph": {
    "title": "HP Document-FrontEnd",
    "type": "website",
    "image": "",
    "site_name": "HP Document-FrontEnd",
    "url": ""
  }
}
